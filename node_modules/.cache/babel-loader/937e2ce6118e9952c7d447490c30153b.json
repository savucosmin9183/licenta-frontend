A,QAAA,YAAA,EAAA;AALA,OAAA;AAOA,WAAA,KAAA,CAAA,IAAA,CAAA,OAAA;AAGA,MAAA,KAAA,CAAA,gCAAA,EAAA;AACA,QAAA,MAAA,EAAA,MADA;AAEA,QAAA,IAAA,EAAA,IAAA,CAAA,SAAA,CAAA;AACA,UAAA,IAAA,EAAA,KAAA,YADA;AAEA,UAAA,EAAA,EAAA,KAAA;AAFA,SAAA;AAFA,OAAA,CAAA,CAOA,IAPA,CAOA,UAAA,QAAA;AAAA,eAAA,QAAA,CAAA,IAAA,EAAA;AAAA,OAPA,EAQA,IARA,CAQA,UAAA,IAAA;AAAA,eAAA,OAAA,CAAA,GAAA,CAAA,IAAA,CAAA;AAAA,OARA;AAWA,WAAA,KAAA,CAAA,UAAA,EAAA,IAAA;AACA;AA5BA,GApCA;AAkEA,EAAA,OAlEA,qBAkEA;AAAA;;AACA,IAAA,KAAA,CAAA,gCAAA,EAAA;AACA,MAAA,MAAA,EAAA,KADA;AAEA,MAAA,OAAA,EAAA;AACA,wBAAA;AADA;AAFA,KAAA,CAAA,CAMA,IANA,CAMA,UAAA,QAAA;AAAA,aAAA,QAAA,CAAA,IAAA,EAAA;AAAA,KANA,EAOA,IAPA,CAOA,UAAA,IAAA,EAAA;AACA,MAAA,IAAA,CAAA,OAAA,CAAA,UAAA,OAAA,EAAA;AACA,QAAA,KAAA,CAAA,KAAA,CAAA,IAAA,CAAA;AACA,UAAA,IAAA,EAAA,OAAA,CAAA,IADA;AAEA,UAAA,UAAA,EAAA,OAAA,CAAA,iBAAA,IAAA,IAAA,GAAA,KAAA,GAAA,OAAA,CAAA,iBAFA;AAGA,UAAA,QAAA,EAAA,OAAA,CAAA,cAAA,IAAA,IAAA,GAAA,KAAA,GAAA,OAAA,CAAA,cAHA;AAIA,UAAA,YAAA,EAAA,OAAA,CAAA,gBAAA,IAAA,IAAA,GAAA,KAAA,GAAA,OAAA,CAAA,gBAJA;AAKA,UAAA,OAAA,EAAA,OAAA,CAAA,MAAA,IAAA,IAAA,GAAA,KAAA,GAAA,OAAA,CAAA;AALA,SAAA;AAOA,OARA;AASA,KAjBA;AAkBA;AArFA,CAAA","sourcesContent":["<template>\n    <div class=\"main_page\">\n        <p class=\"title\">Projects</p>\n        <b-icon icon=\"plus\" class=\"add-icon\" @click=\"show_addProject_modal\"></b-icon>\n        <b-table striped hover :items=\"items\" :fields=\"fields\">\n            <template v-slot:cell(name)=\"data\">\n\n                <a :href=\"`${data.value}`\">{{ data.value }}</a>\n            </template>\n        </b-table>\n        <b-modal size=\"md\" centered ref=\"addModal\" hide-footer title=\"Add a project\">\n            <div class=\"modal_input\">\n                <b-col sm=\"2\">\n                    <label for=\"input-small\">Name</label>\n                </b-col>\n                <b-col sm=\"10\">\n                    <b-form-input id=\"input-small\" @keyup.enter=\"addProject\" v-model=\"project_name\" size=\"sm\" placeholder=\"Enter project name\"></b-form-input>\n                </b-col>\n            </div>\n            <b-button  variant=\"outline-danger\" @click=\"addProject\">Add</b-button>\n        </b-modal>\n    </div>\n</template>\n\n\n<script>\nimport { BIcon } from 'bootstrap-vue'\nexport default {\n    components:{\n         BIcon\n    },\n    data() {\n        return {\n           project_name: '',\n           items: [],\n           fields: [{\n               key: 'name',\n               label: 'Name',\n               sortable: true\n           },\n           {\n               key: 'avg_perf',\n               label: 'Avg. Performance',\n               sortable: true\n           },\n           {\n               key: 'avg_access',\n               label: 'Avg. Accessibillity',\n               sortable: true\n           },\n           {\n               key: 'avg_seo',\n               label: 'Avg. SEO',\n               sortable: true\n           },\n           {\n               key: 'avg_bestprac',\n               label: 'Avg. Best Practices',\n               sortable: true\n           }\n           ]\n        }\n    },\n    methods:{\n        show_addProject_modal(){\n            this.project_name = '';\n            this.$refs['addModal'].show()\n        },\n        addProject(){\n            const project = {\n                name: this.project_name,\n                avg_perf: 'n/a',\n                avg_access: 'n/a',\n                avg_seo: 'n/a',\n                avg_bestprac: 'n/a'\n            }\n            this.items.push(project);\n          \n            \n            fetch(\"http://localhost:1337/projects\", {\n                method: 'POST',\n                body: JSON.stringify({\n                    name: this.project_name,\n                    id: this.project_name\n                })\n            })\n            .then(response => response.json())\n            .then(data => console.log(data))\n            \n\n            this.$refs['addModal'].hide();\n        }\n    },\n    mounted(){\n        fetch(\"http://localhost:1337/projects\", {\n            method: 'GET',\n            headers: {\n                'Content-Type': 'application/json',\n            },\n        })\n        .then(response => response.json())\n        .then(data => {\n            data.forEach(element => {\n                this.items.push({\n                    name: element.name,\n                    avg_access: element.AvgAccessibillity == null ? 'n/a' : element.AvgAccessibillity,\n                    avg_perf: element.AvgPerformance == null ? 'n/a' : element.AvgPerformance,\n                    avg_bestprac: element.AvgBestPractices == null ? 'n/a' : element.AvgBestPractices,\n                    avg_seo: element.AvgSeo == null ? 'n/a' : element.AvgSeo\n                })\n            });\n        })\n    }\n}\n</script>\n\n\n<style>\n    .title{\n        margin-top: 50px;\n        margin-bottom: 30px;\n        margin-left: 20px;\n        font-size: 35px;\n        font-weight: 700;\n        color: black;\n    }\n\n    .main_page{\n        width: 88%;\n        margin-left: 2%;\n    }\n\n    .add-icon{\n        width: 80px;\n        height: 80px;\n        cursor: pointer;\n        float: right;\n        margin-right: 20px;\n        transition: ease-out;\n    }\n\n    .add-icon:hover{\n        transform: scale(1.3);\n    }\n\n    .addmodal{\n        display: flex;\n        flex-direction: column;\n        justify-content: center;\n        align-items: center;\n    }\n\n    .modal_input{\n        display:flex;\n        flex-direction: row;\n        justify-content: center;\n    }\n\n    .modal-body{\n        text-align: center;\n    }\n\n    .btn{\n        margin-top: 20px;\n    }\n\n    .col-sm-2{\n        margin-top: 5px;\n    }\n</style>"],"sourceRoot":"src/components"}]}
KAJA;AAKA,QAAA,QAAA,EAAA;AALA,OAAA;AAQA,WAAA,KAAA,CAAA,IAAA,CAAA,IAAA;AAEA,MAAA,KAAA,CAAA,6BAAA,EAAA;AACA,QAAA,MAAA,EAAA,MADA;AAEA,QAAA,IAAA,EAAA,IAAA,CAAA,SAAA,CAAA;AACA,UAAA,IAAA,EAAA,KAAA,SADA;AAEA,UAAA,OAAA,EAAA;AACA,YAAA,IAAA,EAAA,KAAA,YADA;AAEA,YAAA,EAAA,EAAA;AAFA;AAFA,SAAA;AAFA,OAAA,CAAA,CAUA,IAVA,CAUA,UAAA,QAAA;AAAA,eAAA,QAAA,CAAA,IAAA,EAAA;AAAA,OAVA,EAWA,IAXA,CAWA,UAAA,IAAA;AAAA,eAAA,OAAA,CAAA,GAAA,CAAA,IAAA,CAAA;AAAA,OAXA;AAaA,WAAA,KAAA,CAAA,cAAA,EAAA,IAAA;AACA;AA9BA,GArCA;AAqEA,EAAA,OArEA,qBAqEA;AACA,SAAA,YAAA,GAAA,KAAA,MAAA,CAAA,MAAA,CAAA,YAAA;AACA;AAvEA,CAAA","sourcesContent":["<template>\n    <div class=\"main_page\">\n        <p class=\"title\">{{project_name}}</p>\n        <b-icon icon=\"plus\" class=\"add-icon\" @click=\"show_addLink_modal\"></b-icon>\n        <b-table striped hover :items=\"items\" :fields=\"fields\">\n            <template v-slot:cell(link)=\"data\">\n\n                <a :href=\"`${project_name}/${data.value.replace(/[^a-z1-9]+/i,'-').toLowerCase()}`\">{{ data.value }}</a>\n            </template>\n        </b-table>\n        <b-modal size=\"md\" centered ref=\"addLinkModal\" hide-footer title=\"Add a link\">\n            <div class=\"modal_input\">\n                <b-col sm=\"2\">\n                    <label for=\"input-small\">Link</label>\n                </b-col>\n                <b-col sm=\"10\">\n                    <b-form-input id=\"input-small\" @keyup.enter=\"addLink\" v-model=\"link_name\" size=\"sm\" placeholder=\"Enter link name\"></b-form-input>\n                </b-col>\n            </div>\n            <b-button  variant=\"outline-danger\" @click=\"addLink\">Add</b-button>\n        </b-modal>\n    </div>\n</template>\n\n\n<script>\nimport { BIcon } from 'bootstrap-vue'\nexport default {\n    components:{\n         BIcon\n    },\n    data() {\n        return {\n           project_name: '',\n           link_name: '',\n           items: [],\n           fields: [{\n               key: 'link',\n               label: 'Link',\n               sortable: true\n           },\n           {\n               key: 'perf',\n               label: 'Performance',\n               sortable: true\n           },\n           {\n               key: 'access',\n               label: 'Accessibillity',\n               sortable: true\n           },\n           {\n               key: 'seo',\n               label: 'SEO',\n               sortable: true\n           },\n           {\n               key: 'bestprac',\n               label: 'Best Practices',\n               sortable: true\n           }\n           ]\n        }\n    },\n    methods:{\n        show_addLink_modal(){\n            this.link_name = '';\n            this.$refs['addLinkModal'].show()\n        },\n        addLink(){\n            const link = {\n                link: this.link_name,\n                perf: 'n/a',\n                access: 'n/a',\n                seo: 'n/a',\n                bestprac: 'n/a'\n            }\n\n            this.items.push(link);\n            \n            fetch(\"http://localhost:1337/links\", {\n                method: 'POST',\n                body: JSON.stringify({\n                    name: this.link_name,\n                    project: {\n                        name: this.project_name,\n                        id: 1\n                    }\n                })\n            })\n            .then(response => response.json())\n            .then(data => console.log(data))\n            \n            this.$refs['addLinkModal'].hide();\n        }\n    },\n    mounted(){\n        this.project_name = this.$route.params.project_name;\n    }\n}\n</script>\n\n\n<style>\n\n</style>"],"sourceRoot":"src/views"}]}